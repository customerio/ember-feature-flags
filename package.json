{
  "name": "ember-feature-flags",
  "version": "6.1.0",
  "description": "Feature flags/toggles for Ember apps",
  "scripts": {
    "build": "ember build --environment=production",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"yarn:lint:css -- --fix\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "changelog": "lerna-changelog"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^4.0.5",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.19.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.2.2",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^9.0.1",
    "ember-resolver": "^13.1.0",
    "ember-source": "~6.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-try": "^3.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.14.0",
    "lerna-changelog": "^0.8.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.3",
    "webpack": "^5.97.1"
  },
  "keywords": [
    "ember-addon",
    "feature-flag",
    "feature-toggle",
    "features-switch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kategengler/ember-feature-flags"
  },
  "license": "MIT",
  "author": "Katie Gengler",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@babel/core": "^7.26.7",
    "ember-cli-babel": "^8.2.0",
    "ember-template-imports": "^4.3.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=1.12.1"
    }
  },
  "changelog": {
    "repo": "kategengler/ember-feature-flags",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  }
}

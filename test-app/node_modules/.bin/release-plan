#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules/release-plan/dist/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules/release-plan/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules/release-plan/dist/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules/release-plan/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/release-plan@0.13.1/node_modules:/Users/yegor/code/ember-feature-flags/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../release-plan/dist/cli.js" "$@"
else
  exec node  "$basedir/../release-plan/dist/cli.js" "$@"
fi
